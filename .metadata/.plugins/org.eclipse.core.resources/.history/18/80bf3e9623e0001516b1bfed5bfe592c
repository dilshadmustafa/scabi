package com.dilmus.dilshad.scabi.client.async;

import org.apache.http.concurrent.FutureCallback;

/**
 * @author Dilshad Mustafa
 *
 */
public class DFutureCallback<HttpResponse> implements FutureCallback<HttpResponse> {

	public DComputeNoBlock m_cnb = null;
	
	public DFutureCallback(DComputeNoBlock cnb) {
		m_cnb = cnb;
	}
	
    public void completed(final HttpResponse response) {
        //latch.countDown();
        System.out.println("completed");
    }

    public void failed(final Exception ex) {
        //latch.countDown();
        System.out.println("failed");
    }

    public void cancelled() {
        latch.countDown();
        System.out.println("cancelled");
    }

}
